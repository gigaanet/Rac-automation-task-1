starting_value = int(input ("Please, Enter a starting number "))  
ending_value = int(input ("Please, Enter a ending number "))  
  
print ("The Prime Numbers in the selected range")
for number in range (starting_value, ending_value + 1):  
    if number > 1:  
        for i in range (2, number):  
            if (number % i) == 0:  
                break  
        else:  
            print (number)  
